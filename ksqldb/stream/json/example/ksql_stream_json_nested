CREATE STREAM TRANSAKSI_EMAS_01 (
  message STRUCT<
    data STRUCT<
      ID BIGINT,
      ID_DETAIL BIGINT,
      AMOUNT DOUBLE,
      BERAT INT,
      BIAYA DOUBLE,
      JUMLAH INT
    >,
    headers STRUCT<
      operation VARCHAR
    >
  >
) 
WITH (
  KAFKA_TOPIC='TRANSAKSI_EMAS',
  VALUE_FORMAT='JSON',
  PARTITIONS=10
);
